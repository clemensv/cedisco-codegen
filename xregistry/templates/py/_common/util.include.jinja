{%- macro DeclareDataType(project_name, root, message) -%}
{%- if message.schemauri or message.schema -%}
{%- set dataType = ((message.schemauri if message.schemauri else message.schema) | schema_type( project_name, root, message.schemaformat) ) -%}
{%- set dataType = ( dataType | namespace | lower )+'.'+(dataType | strip_namespace | pascal) %}
{%- else -%}
{%- set dataType = "object" -%}
{%- endif -%}
{{ dataType }}
{%- endmacro -%}