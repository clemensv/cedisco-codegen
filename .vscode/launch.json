      "preLaunchTask": "tsc: build - xregistry_vscode/tsconfig.json",
      "outFiles": ["${workspaceFolder}/out/**/*.js"],
      "request": "launch",
      "type": "extensionHost"
    },
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
